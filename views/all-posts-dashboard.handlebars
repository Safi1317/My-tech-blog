
<div class="posts col">
{{#each posts}}
		<article class="card">
			<div class="card-header flex-row justify-space-between">
				<h2>
					<a href="/dashboard/edit/{{id}}">{{title}}</a>
				</h2>
				<div>
				Posted by
					{{User.username}}
					on
					{{format_date createdAt}}
				</div>
			</div>
			<div class="card-body">
				{{content}}
			</div>
		</article>
	{{>new-post}}
	{{/each}}
</div>


